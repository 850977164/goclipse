<project name="download_target" default="dl_eclipse_36" basedir=".">

    <target name="dl_eclipse_36">
        <property environment="env" />
    
        <echo message="build=${env.BUILD_TAG}"/>
    
        <property name="workspace" location="${env.WORKSPACE}"/>
        
        <get src="http://archive.eclipse.org/eclipse/downloads/drops/R-3.6.2-201102101200/eclipse-SDK-3.6.2-win32.zip"
            dest="${workspace}" 
            verbose="true"
            usetimestamp="true"/>
    
        <unzip src="${workspace}/eclipse-SDK-3.6.2-win32.zip" dest="${workspace}/eclipse_36" overwrite="false"/>    
    </target>

</project>
